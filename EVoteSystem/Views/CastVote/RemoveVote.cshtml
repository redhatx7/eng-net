@model EVoteSystem.Models.Vote
@{
    ViewBag.Title = "title";
    Layout = "_Layout";
}


<div class="row">
    <div class="col-md-6">
        <!-- general form elements -->
        <div class="card card-info">
            <div class="card-header">
                <h3 class="card-title">افزودن این دانش آموز به لیست کاندید ها</h3>
            </div>
            <!-- /.card-header -->
            <!-- form start -->
            <form asp-action="RemoveVote" enctype="multipart/form-data">
                <div class="form-group ">
                    <label asp-for="ToCandidate.Student.Fullname" class="control-label col-lg-2"></label>
                    <div class="col-lg-10">
                        <input class="form-control" asp-for="ToCandidate.Student.Fullname" readonly/>
                    </div>
                </div>
                <div class="form-group ">
                    <label asp-for="CreatedAt" class="control-label col-lg-2"></label>
                    <div class="col-lg-10">
                        <input class="form-control" asp-for="CreatedAt" readonly/>
                    </div>
                </div>


                <!-- /.card-body -->

                <div class="card-footer">
                    <button type="submit" class="btn btn-danger">حذف</button>
                </div>
            </form>
        </div>


    </div>
</div>